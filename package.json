{
  "name": "superskills",
  "version": "0.1.0",
  "description": "AI-native product development assistant - Skills that amplify human capabilities",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "superskills": "./bin/superskills"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && cp src/catalog/tools-catalog.json dist/catalog/",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch --clear-screen=false --ignore 'dist/**' --ignore 'node_modules/**' src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/",
    "check-sync": "bash scripts/check-sync.sh",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Play-New/SuperSkills.git"
  },
  "bugs": {
    "url": "https://github.com/Play-New/SuperSkills/issues"
  },
  "homepage": "https://github.com/Play-New/SuperSkills",
  "keywords": [
    "ai-native",
    "product-development",
    "discovery",
    "cli",
    "ai",
    "claude",
    "skills"
  ],
  "author": "Play New",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.76.0",
    "@clack/prompts": "^1.0.1",
    "commander": "^14.0.3",
    "fs-extra": "^11.2.0",
    "picocolors": "^1.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9.39.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
